{
  "name": "nssports",
  "description": "NSSPORTS Next.js application",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "author": "Elliott Miller",
  "repository": {
    "type": "git",
    "url": "https://github.com/elliotttmiller/NSSPORTS"
  },
  "homepage": "https://github.com/elliotttmiller/NSSPORTS#readme",
  "bugs": {
    "url": "https://github.com/elliotttmiller/NSSPORTS/issues"
  },
  "engines": {
    "node": ">=18.18.0",
    "npm": ">=10.0.0"
  },
  "packageManager": "npm@10.9.0",
  "scripts": {
    "predev": "prisma generate",
    "dev": "next dev --turbopack",
    "prebuild": "node scripts/prebuild.mjs",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "postinstall": "prisma generate",
    "init": "node scripts/init-system.mjs",
    "verify": "node scripts/verify-db.mjs",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force",
    "cache:clear": "tsx scripts/clear-cache.ts",
    "settle-bets": "tsx scripts/settlement/settle-bets.ts",
    "settle-bets:dry-run": "tsx scripts/settlement/settle-bets.ts --dry-run",
    "settlement:start": "tsx scripts/start-settlement.ts",
    "settlement:init": "tsx src/scripts/init-settlement-scheduler.ts",
    "settlement:worker": "tsx src/workers/settlement-worker.ts",
    "settlement:trigger": "tsx trigger-settlement.ts",
    "settlement:test": "tsx test-settlement.ts",
    "settlement:test:comprehensive": "tsx scripts/testing/comprehensive-bet-settlement-test-v2.ts",
    "settlement:manual": "tsx manual-settlement.ts",
    "test:settlement": "tsx src/scripts/test-settlement-dry-run.ts",
    "test:settlement:verbose": "tsx src/scripts/test-settlement-dry-run.ts --verbose",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rimraf .next node_modules/.cache",
    "reinstall": "rimraf node_modules package-lock.json && npm install"
  },
  "dependencies": {
    "@phosphor-icons/react": "^2.1.10",
    "@prisma/client": "^6.19.0",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@studio-freight/lenis": "^1.0.42",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-virtual": "^3.13.12",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.63.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.22",
    "gsap": "^3.13.0",
    "ioredis": "^5.8.2",
    "lenis": "^1.3.13",
    "lucide-react": "^0.544.0",
    "next": "^15.1.3",
    "next-auth": "^5.0.0-beta.29",
    "pusher-js": "^8.4.0-rc2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sonner": "^2.0.7",
    "sports-odds-api": "^1.2.1",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.76",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.14",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.18.13",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "babel-jest": "^30.0.0",
    "depcheck": "^1.4.7",
    "eslint": "^9",
    "eslint-config-next": "^15.1.3",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^30.0.0",
    "prettier": "^3.3.3",
    "prisma": "^6.19.0",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "typescript": "^5"
  },
  "prisma": {
    "seed": "tsx ./prisma/seed.ts"
  },
  "keywords": [
    "nextjs",
    "react",
    "prisma",
    "tailwindcss",
    "nssports"
  ]
}
